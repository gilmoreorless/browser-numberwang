{
    "name": "Numberwang",
    "version": "0.0.1",
    "description": "Tells you when a number you've typed is numberwang",
    "manifest_version": 2,
    "icons": {},
    "background": {
        "scripts": ["background.js"]
    },
    "content_scripts": [{
        "matches": ["http://*/*", "https://*/*"],
        "css": ["numberwang.css"],
        "js": ["numberwang.js"]
    }],
    "permissions": [
        "*://*/*",
        "notifications"
    ]
}
